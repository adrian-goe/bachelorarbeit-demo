  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve",
    "start:api": "nx serve api",
    "start:mono": "nx serve tunierplanner-mono",
    "start:mf-shell": "nx serve mf-shell",
    "start:teams": "nx serve teams",
    "start:matches": "nx serve matches",
    "start:tunier": "nx serve tunier",
    "build:all": "npm run build:mono && npm run build:mf-shell && npm run build:teams && npm run build:matches && npm run build:tunier ",
    "build:mono": "NODE_ENV=production ng build tunierplanner-mono --configuration production",
    "build:mf-shell": "NODE_ENV=production ng build mf-shell --configuration production",
    "build:teams": "NODE_ENV=production ng build teams --configuration production",
    "build:matches": "NODE_ENV=production ng build matches --configuration production",
    "build:tunier": "NODE_ENV=production ng build tunier --configuration production",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "build:prod": "cross-env NODE_ENV=production ng build --configuration production",
    "postinstall": "node ./decorate-angular-cli.js && if [ -n \"$SKIP_IVY\" ] ; then exit ; fi && npm run compile-for-ivy && ngcc --properties es2015 browser module main",
    "compile-for-ivy": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
